          "id": "info-col-2",
          "elType": "column",
          "settings": {
            "_column_size": 33,
            "background_background": "classic",
            "background_color": "#F8F9FA",
            "border_radius": {"unit": "px", "top": "10", "right": "10", "bottom": "10", "left": "10", "isLinked": true},
            "padding": {"unit": "px", "top": "40", "right": "30", "bottom": "40", "left": "30", "isLinked": false},
            "margin": {"unit": "px", "top": "0", "right": "15", "bottom": "0", "left": "15", "isLinked": false}
          },
          "elements": [
            {
              "id": "info-icon-2",
              "elType": "widget",
              "settings": {
                "selected_icon": {"value": "fas fa-envelope", "library": "fa-solid"},
                "align": "center",
                "primary_color": "#16A3BD",
                "icon_size": {"unit": "px", "size": 40}
              },
              "widgetType": "icon"
            },
            {
              "id": "info-title-2",
              "elType": "widget",
              "settings": {
                "title": "Email Us",
                "header_size": "h4",
                "align": "center",
                "title_color": "#1E1666",
                "typography_font_size": {"unit": "px", "size": 20},
                "typography_font_weight": "600",
                "_margin": {"unit": "px", "top": "15", "right": "0", "bottom": "10", "left": "0", "isLinked": false}
              },
              "widgetType": "heading"
            },
            {
              "id": "info-text-2",
              "elType": "widget",
              "settings": {
                "editor": "<p style=\"text-align: center;\"><a href=\"mailto:design@redrocket.digital\">design@redrocket.digital</a></p><p style=\"text-align: center;\">We typically respond within 24 hours</p>",
                "text_color": "#6A7C92",
                "typography_font_size": {"unit": "px", "size": 15}
              },
              "widgetType": "text-editor"
            }
          ]
        },
        {
          "id": "info-col-3",
          "elType": "column",
          "settings": {
            "_column_size": 33,
            "background_background": "classic",
            "background_color": "#F8F9FA",
            "border_radius": {"unit": "px", "top": "10", "right": "10", "bottom": "10", "left": "10", "isLinked": true},
            "padding": {"unit": "px", "top": "40", "right": "30", "bottom": "40", "left": "30", "isLinked": false},
            "margin": {"unit": "px", "top": "0", "right": "15", "bottom": "0", "left": "15", "isLinked": false}
          },
          "elements": [
            {
              "id": "info-icon-3",
              "elType": "widget",
              "settings": {
                "selected_icon": {"value": "fas fa-clock", "library": "fa-solid"},
                "align": "center",
                "primary_color": "#FFA526",
                "icon_size": {"unit": "px", "size": 40}
              },
              "widgetType": "icon"
            },
            {
              "id": "info-title-3",
              "elType": "widget",
              "settings": {
                "title": "Opening Hours",
                "header_size": "h4",
                "align": "center",
                "title_color": "#1E1666",
                "typography_font_size": {"unit": "px", "size": 20},
                "typography_font_weight": "600",
                "_margin": {"unit": "px", "top": "15", "right": "0", "bottom": "10", "left": "0", "isLinked": false}
              },
              "widgetType": "heading"
            },
            {
              "id": "info-text-3",
              "elType": "widget",
              "settings": {
                "editor": "<p style=\"text-align: center;\">Monday - Friday<br>9:00 AM - 5:30 PM</p>",
                "text_color": "#6A7C92",
                "typography_font_size": {"unit": "px", "size": 15}
              },
              "widgetType": "text-editor"
            }
          ]
        }
      ]
    },
    {
      "id": "form-section",
      "elType": "section",
      "settings": {
        "layout": "boxed",
        "content_width": {"unit": "px", "size": 1140},
        "padding": {"unit": "px", "top": "60", "right": "20", "bottom": "80", "left": "20", "isLinked": false}
      },
      "elements": [
        {
          "id": "form-col-text",
          "elType": "column",
          "settings": {"_column_size": 50, "_inline_size": 50, "padding": {"unit": "px", "top": "0", "right": "40", "bottom": "0", "left": "0", "isLinked": false}},
          "elements": [
            {
              "id": "form-title",
              "elType": "widget",
              "settings": {
                "title": "Send Us a Message",
                "header_size": "h2",
                "title_color": "#1E1666",
                "typography_font_family": "Poppins",
                "typography_font_size": {"unit": "px", "size": 32},
                "typography_font_weight": "600"
              },
              "widgetType": "heading"
            },
            {
              "id": "form-desc",
              "elType": "widget",
              "settings": {
                "editor": "<p>Fill out the form below and we'll get back to you as soon as possible. Whether you have a question about our services, need a quote, or just want to say hello, we're here to help.</p>",
                "text_color": "#6A7C92",
                "typography_font_size": {"unit": "px", "size": 15},
                "typography_line_height": {"unit": "px", "size": 28},
                "_margin": {"unit": "px", "top": "20", "right": "0", "bottom": "30", "left": "0", "isLinked": false}
              },
              "widgetType": "text-editor"
            },
            {
              "id": "social-icons",
              "elType": "widget",
              "settings": {
                "social_icon_list": [
                  {"social_icon": {"value": "fab fa-facebook-f", "library": "fa-brands"}, "link": {"url": "#"}},
                  {"social_icon": {"value": "fab fa-twitter", "library": "fa-brands"}, "link": {"url": "#"}},
                  {"social_icon": {"value": "fab fa-linkedin-in", "library": "fa-brands"}, "link": {"url": "#"}},
                  {"social_icon": {"value": "fab fa-instagram", "library": "fa-brands"}, "link": {"url": "#"}}
                ],
                "icon_color": "custom",
                "icon_primary_color": "#1E1666",
                "icon_secondary_color": "#FFFFFF",
                "icon_size": {"unit": "px", "size": 16},
                "icon_spacing": {"unit": "px", "size": 15}
              },
              "widgetType": "social-icons"
            }
          ]
        },
        {
          "id": "form-col-form",
          "elType": "column",
          "settings": {
            "_column_size": 50,
            "_inline_size": 50,
            "background_background": "classic",
            "background_color": "#F8F9FA",
            "border_radius": {"unit": "px", "top": "10", "right": "10", "bottom": "10", "left": "10", "isLinked": true},
            "padding": {"unit": "px", "top": "40", "right": "40", "bottom": "40", "left": "40", "isLinked": true}
          },
          "elements": [
            {
              "id": "form-note",
              "elType": "widget",
              "settings": {
                "editor": "<p><strong>Note:</strong> To add a contact form, install a form plugin like <strong>WPForms</strong> or <strong>Contact Form 7</strong>, then replace this text with your form shortcode.</p><p>Example shortcode:</p><p><code>[wpforms id=\"123\"]</code></p><p>Or use <strong>Elementor Pro</strong> which includes a built-in Form widget.</p>",
                "text_color": "#6A7C92",
                "typography_font_size": {"unit": "px", "size": 14},
                "_padding": {"unit": "px", "top": "20", "right": "20", "bottom": "20", "left": "20", "isLinked": true},
                "_background_background": "classic",
                "_background_color": "#FFFFFF",
                "_border_radius": {"unit": "px", "top": "5", "right": "5", "bottom": "5", "left": "5", "isLinked": true}
              },
              "widgetType": "text-editor"
            }
          ]
        }
      ]
    },
    {
      "id": "map-section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "padding": {"unit": "px", "top": "0", "right": "0", "bottom": "0", "left": "0", "isLinked": true}
      },
      "elements": [
        {
          "id": "map-col",
          "elType": "column",
          "settings": {"_column_size": 100},
          "elements": [
            {
              "id": "google-map",
              "elType": "widget",
              "settings": {
                "address": "Ransomes Euro Park, Ipswich, Suffolk IP3 9FJ, UK",
                "zoom": {"unit": "px", "size": 15},
                "height": {"unit": "px", "size": 450}
              },
              "widgetType": "google_maps"
            }
          ]
        }
      ]
    }
