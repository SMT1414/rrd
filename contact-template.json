{
  "version": "0.4",
  "title": "Red Rocket Contact Page",
  "type": "page",
  "content": [
    {
      "id": "contact-hero",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "padding": {"unit": "px", "top": "100", "right": "20", "bottom": "100", "left": "20", "isLinked": false},
        "background_background": "gradient",
        "background_color": "#1E1666",
        "background_color_b": "#2A1F7A"
      },
      "elements": [
        {
          "id": "hero-col",
          "elType": "column",
          "settings": {"_column_size": 100},
          "elements": [
            {
              "id": "hero-heading",
              "elType": "widget",
              "settings": {
                "title": "Get In Touch",
                "header_size": "h1",
                "align": "center",
                "title_color": "#FFFFFF",
                "typography_font_family": "Poppins",
                "typography_font_size": {"unit": "px", "size": 48},
                "typography_font_weight": "600"
              },
              "widgetType": "heading"
            },
            {
              "id": "hero-subtitle",
              "elType": "widget",
              "settings": {
                "editor": "<p style=\"text-align: center;\">We'd love to hear from you. Let's start a conversation.</p>",
                "text_color": "rgba(255,255,255,0.8)",
                "typography_font_size": {"unit": "px", "size": 18}
              },
              "widgetType": "text-editor"
            }
          ]
        }
      ]
    },
    {
      "id": "contact-info-section",
      "elType": "section",
      "settings": {
        "layout": "boxed",
        "content_width": {"unit": "px", "size": 1140},
        "padding": {"unit": "px", "top": "80", "right": "20", "bottom": "40", "left": "20", "isLinked": false}
      },
      "elements": [
        {
          "id": "info-col-1",
          "elType": "column",
          "settings": {
            "_column_size": 33,
            "background_background": "classic",
            "background_color": "#F8F9FA",
            "border_radius": {"unit": "px", "top": "10", "right": "10", "bottom": "10", "left": "10", "isLinked": true},
            "padding": {"unit": "px", "top": "40", "right": "30", "bottom": "40", "left": "30", "isLinked": false},
            "margin": {"unit": "px", "top": "0", "right": "15", "bottom": "0", "left": "15", "isLinked": false}
          },
          "elements": [
            {
              "id": "info-icon-1",
              "elType": "widget",
              "settings": {
                "selected_icon": {"value": "fas fa-map-marker-alt", "library": "fa-solid"},
                "align": "center",
                "primary_color": "#EC5D49",
                "icon_size": {"unit": "px", "size": 40}
              },
              "widgetType": "icon"
            },
            {
              "id": "info-title-1",
              "elType": "widget",
              "settings": {
                "title": "Visit Us",
                "header_size": "h4",
                "align": "center",
                "title_color": "#1E1666",
