{
  "version": "0.4",
  "title": "Red Rocket Header",
  "type": "header",
  "content": [
    {
      "id": "header-section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "content_width": {"unit": "px", "size": 1200},
        "padding": {"unit": "px", "top": "15", "right": "30", "bottom": "15", "left": "30", "isLinked": false},
        "background_background": "classic",
        "background_color": "#FFFFFF",
        "box_shadow_box_shadow_type": "yes",
        "box_shadow_box_shadow": {"horizontal": 0, "vertical": 2, "blur": 10, "spread": 0, "color": "rgba(0,0,0,0.08)"},
        "stretch_section": "section-stretched"
      },
      "elements": [
        {
          "id": "logo-col",
          "elType": "column",
          "settings": {"_column_size": 25, "_inline_size": 25},
          "elements": [
            {
              "id": "site-logo",
              "elType": "widget",
              "settings": {
                "image": {"url": "", "id": ""},
                "image_size": "medium",
                "align": "left",
                "link_to": "custom",
                "link": {"url": "/"},
                "width": {"unit": "px", "size": 180}
              },
              "widgetType": "image"
            }
          ]
        },
        {
          "id": "nav-col",
          "elType": "column",
          "settings": {"_column_size": 55, "_inline_size": 55},
          "elements": [
            {
              "id": "nav-menu",
              "elType": "widget",
              "settings": {
                "menu": "primary",
                "layout": "horizontal",
                "align_items": "center",
                "pointer": "underline",
                "text_color": "#1E1666",
                "text_color_hover": "#EC5D49",
                "typography_typography": "custom",
                "typography_font_family": "Roboto",
                "typography_font_size": {"unit": "px", "size": 15},
                "typography_font_weight": "500"
              },
              "widgetType": "nav-menu"
            }
          ]
        },
        {
          "id": "cta-col",
          "elType": "column",
          "settings": {"_column_size": 20, "_inline_size": 20},
          "elements": [
            {
              "id": "header-btn",
              "elType": "widget",
              "settings": {
                "text": "Get Quote",
                "link": {"url": "/contact-us/"},
                "align": "right",
                "background_color": "#FF6C26",
                "button_background_hover_color": "#FFA526",
                "border_radius": {"unit": "px", "top": "5", "right": "5", "bottom": "5", "left": "5", "isLinked": true},
                "button_text_padding": {"unit": "px", "top": "10", "right": "25", "bottom": "10", "left": "25", "isLinked": false},
                "typography_font_size": {"unit": "px", "size": 14},
                "typography_font_weight": "500"
              },
              "widgetType": "button"
            }
          ]
        }
      ]
    }
  ],
  "page_settings": {}
}
